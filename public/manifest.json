{
    "name": "시대링크",
    "description": "시대링크: 한 눈에 확인하는 서울시립대",
    "manifest_version": 3,
    "version": "0.9.3",
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "128": "icons/128.png"
    },
    "offline_enabled": true,
    "chrome_url_overrides" : {
        "newtab": "index.html"
    },
    "action": {
        "default_icon": {
            "16": "icons/16.png",
            "32": "icons/32.png",
            "48": "icons/48.png",
            "128": "icons/128.png"
        },
        "default_title": "시대링크"
    }
}
